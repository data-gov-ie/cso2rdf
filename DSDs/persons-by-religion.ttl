@prefix qb:             <http://purl.org/linked-data/cube#> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix sdmx-concept:   <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .

# --- Own namespaces ------------------------

@prefix dsd:            <http://stats.govdata.ie/dsd/> .
@prefix prop:           <http://stats.govdata.ie/property/> .
@prefix code:           <http://stats.govdata.ie/codelist/> .
@prefix concept:        <http://stats.govdata.ie/concept/> .

# --- Data structure definition -------------

dsd:persons-by-religion a qb:DataStructureDefinition;
    qb:component [
            a qb:ComponentSpecification;
            rdfs:label "Year"@en;
            qb:dimension sdmx-dimension:refPeriod;
            qb:order "1"^^xsd:int
        ], [
            a qb:ComponentSpecification;
            qb:dimension sdmx-dimension:refArea;
            qb:order "2"^^xsd:int
        ], [
            a qb:ComponentSpecification;
            qb:dimension prop:religion;
            qb:order "3"^^xsd:int
        ], [
            a qb:ComponentSpecification;
            qb:measure prop:population;
            qb:order "4"^^xsd:int
        ]
    .
          
# --- Property specifications ---------------

prop:geoArea a qb:DimensionProperty;
    rdfs:label "Geographic area"@en;
    rdfs:subPropertyOf sdmx-dimension:refArea;
    qb:concept sdmx-concept:refArea;
    qb:codeList code:geo
    .
    
prop:religion a qb:DimensionProperty;
    rdfs:label "Religion"@en;
    qb:concept concept:religion;
    qb:codeList code:religion
    .

prop:population a qb:MeasureProperty;
    rdfs:label "Population"@en;
    rdfs:subPropertyOf qb:obsValue;
    qb:concept sdmx-concept:statPop;
    rdfs:range xsd:int
    .
