@prefix rdf:                <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdmx:               <http://purl.org/linked-data/sdmx#> .
@prefix skos:               <http://www.w3.org/2004/02/skos/core#> .

# --- Own namespaces ------------------------

@prefix code:               <http://stats.govdata.ie/codelist/> .
@prefix code-nationality:   <http://stats.govdata.ie/codelist/nationality/> .

#Â --- Code lists ----------------------------
  
code:nationality a sdmx:CodeList;
    skos:prefLabel "Code list for nationalities"@en;
    skos:hasTopConcept code-nationality:irish, code-nationality:uk, code-nationality:polish, code-nationality:lithuanian, code-nationality:other-eu-25, code-nationality:rest
    .
    
code-nationality:irish a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "Irish"@en;
    skos:closeMatch <http://dbpedia.org/resource/Irish_people>;
    skos:notation "irish"
    .
    
code-nationality:uk a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "UK"@en;
    skos:closeMatch <http://dbpedia.org/resource/British_people>;
    skos:notation "uk"
    .
    
code-nationality:polish a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "Polish"@en;
    skos:closeMatch <http://dbpedia.org/resource/Poles>;
    skos:notation "polish"
    .
    
code-nationality:lithuanian a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "Lithuanian"@en;
    skos:closeMatch <http://dbpedia.org/resource/Lithuanian_people>;
    skos:notation "lithuanian"
    .
    
code-nationality:other-eu-25 a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "Other EU25"@en;
    skos:notation "other-eu-25"
    .
    
code-nationality:rest a sdmx:Concept;
    skos:topConceptOf code:nationality;
    skos:prefLabel "Rest of World"@en;
    skos:notation "rest"
    .
