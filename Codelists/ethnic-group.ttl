@prefix rdf:                <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdmx:               <http://purl.org/linked-data/sdmx#> .
@prefix skos:               <http://www.w3.org/2004/02/skos/core#> .

# --- Own namespaces ------------------------

@prefix code:               <http://stats.govdata.ie/codelist/> .
@prefix code-ethicgroup:    <http://stats.govdata.ie/codelist/ethnic-group/> .

#Â --- Code lists ----------------------------
  
code:ethnic-group a sdmx:CodeList;
    skos:prefLabel "Code list for ethnic groups"@en;
    skos:hasTopConcept code-ethicgroup:white-irish, code-ethicgroup:white-irish-traveller, code-ethicgroup:other-white, code-ethicgroup:black, code-ethicgroup:asian, code-ethicgroup:other, code-ethicgroup:not-stated
    .
    
code-ethicgroup:white-irish a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "White Irish"@en;
    skos:notation "white-irish";
    .
    
code-ethicgroup:white-irish-traveller a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "White Irish Traveller"@en;
    skos:notation "white-irish-traveller";
    .
    
code-ethicgroup:other-white a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "Any other White background"@en;
    skos:notation "other-white";
    .
    
code-ethicgroup:black a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "Black or Black Irish"@en;
    skos:notation "black";
    .
    
code-ethicgroup:asian a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "Asian or Asian Irish"@en;
    skos:notation "asian"
    .
    
code-ethicgroup:other a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "Other"@en;
    skos:notation "other"
    .
    
code-ethicgroup:not-stated a skos:Concept;
    skos:inScheme code:ethnic-group;
    skos:topConceptOf code:ethnic-group;
    skos:prefLabel "Not stated"@en;
    skos:notation "not-stated"
    .
